<service>
	<id>nodeserver.exe</id>
	<name>nodeServer</name>
	<description>This is Backend Node.js service</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>--harmony</argument>
	<argument>C:\Backend\BIGCRM-API\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\Backend\BIGCRM-API\index.js</argument>
	<argument>--scriptoptions=</argument>
	<argument>--log</argument>
	<argument>nodeServer wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<logmode>rotate</logmode>
	<stoptimeout>30sec</stoptimeout>
	<serviceaccount>
		<domain>BIGCRM</domain>
		<user>LocalSystem</user>
		<password></password>
	</serviceaccount>
	<workingdirectory>C:\Backend\BIGCRM-API</workingdirectory>
</service>